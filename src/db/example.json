		{
			"command"     : "find . -name \"*.txt\"",
			"description" : "Finds all files cotaining string %str%",
			"nix_edit"    : {
								"mask": "find . -name \"%s\"",
								"args": ["*.txt"]
							},
			"tag"         : ["find", "files", "contain"]
		},
